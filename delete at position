#include<stdio.h>
#define CAP 50
int main(){
    int a[CAP]={100,400,600,550,800,200};
    int n=6;
    int pos=3;
    {
        printf("name:poojashree s,usn:cs097\n");
    }
    if(pos<0 || pos>=n){
          printf("invalid");
        }
    else{
        for(int i=pos;i<n-1;i++){
            a[i]=a[i+1];
        }
        n--;
    }
    for(int i=0;i<n;i++){
        printf("%d ",a[i]);
    }
}
