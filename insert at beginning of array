#include<stdio.h>
#define CAP 10
int main(){
    int a[CAP]={1,3,5,7};
    int n=4;
    int x=23;
    if(n<CAP){
        for(int i=n;i>n;i--){
            a[i]=a[i-1];
        }
        a[0]=x;
        n++;
    }
    else{
        printf("array is filled");
    }
    for(int i=0;i<n;i++){
        printf("%d ",a[i]);
    }
}
