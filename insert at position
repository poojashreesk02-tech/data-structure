int main(){
    int a[]={10,40,60,50,80,20};
    int n=6;
    int pos=3;
    int newvariable=70;
    {
        printf("name:poojashree s,usn:cs097\n");
    }
    if(n>pos){
          for(int i=n;i>pos-1;i--){
              a[i]=a[i-1];
          }
          a[pos]=newvariable;
          n++;
        }
    else{
        printf("good day");
        }
        for(int i=0;i<n;i++){
            printf("%d ",a[i]);
        }

