#include<stdio.h>
#include<stdlib.h>
struct node{
    int data;
    struct node* left;
    struct node* right;
};
struct node* createnode(int value){
    struct node* newnode=malloc(sizeof(struct node));
    newnode->data=value;
    newnode->left=NULL;
    newnode->right=NULL;
    return newnode;
}
void preorder(struct node* root){
    if(root==NULL)
    return;
    printf("%d",root->data);
    preorder(root->left);
    preorder(root->right);
}
